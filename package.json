{
  "name": "telligy-server",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "build": "tsc",
    "db:migrate": "tsx -r dotenv/config src/migrate.ts",
    "db:generate": "bunx drizzle-kit generate:pg",
    "db:introspect": "bunx drizzle-kit introspect:pg"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "drizzle-kit": "^0.20.17",
    "express": "^4.19.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.10.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.9",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.4",
    "tsx": "^4.7.2",
    "zod": "^3.23.0"
  }
}